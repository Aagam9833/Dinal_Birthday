<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dinalsbirthday">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bdayicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bdayicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.DinalsBirthday">
        <activity
            android:name=".Youtube"
            android:screenOrientation="landscape" />
        <activity android:name=".Maps" android:screenOrientation="portrait"/>
        <activity android:name=".Gallery" android:screenOrientation="portrait"/>
        <activity android:name=".Inshorts" android:screenOrientation="portrait"/>
        <activity android:name=".Quiz" android:screenOrientation="portrait"/>
        <activity android:name=".HomeScreen" android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>